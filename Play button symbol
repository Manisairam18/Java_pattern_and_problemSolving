Program:

class HelloWorld {
    public static void main(String[] args) {
int n=5;
for(int row=1;row<=n*2-1;row++){
    int spaces = row<=n ? row:n*2-row;
    for(int col=1;col<=spaces;col++){
        System.out.print("* ");
    }
    System.out.println();
}

    }
}
